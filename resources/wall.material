// wall material definition
material wall
  {
    // define one technique for rendering a wall
    technique
     {
       // render a wall in one pass
       pass
        {
          // color the wall cyan
          ambient 0 0.545 0.545
          diffuse 0 0.545 0.545
          specular 0 0.545 0.545 120
        }
     }
  }